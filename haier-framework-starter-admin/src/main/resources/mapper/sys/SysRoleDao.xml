<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.haier.fintech.modules.sys.dao.SysRoleDao">

	<!-- 查询用户创建的角色ID列表 -->
	<select id="queryRoleIdList" resultType="long">
		select role_id from
		sys_role where create_user_id = #{createUserId}
	</select>
	
	<select id="getRolesList" resultType="com.haier.fintech.modules.sys.entity.SysRoleEntity">
		select * from
		sys_role where role_id !=1 and role_id !=8 
	</select>
</mapper>